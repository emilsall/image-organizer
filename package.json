{
  "name": "media-organizer",
  "version": "1.0.6",
  "description": "Organize and deduplicate photos, RAW files, and videos by date",
  "type": "module",
  "bin": {
    "media-organizer": "./media-organizer.ts"
  },
  "scripts": {
    "start": "bun run media-organizer.ts",
    "build": "bun run scripts/build.ts",
    "typecheck": "bun x tsc -p tsconfig.json --noEmit",
    "dist:clean": "rm -rf dist && mkdir -p dist"
  },
  "keywords": [
    "photos",
    "organize",
    "exif",
    "raw",
    "canon",
    "fujifilm",
    "media"
  ],
