  "dependencies": {
    "ink": "^6.6.0",
    "ink-spinner": "^5.0.0",
    "react": "^19.2.3",
    "ink-scroll-list": "^0.4.1",
    "ink-text-input": "^6.0.0",
    "react-devtools-core": "^5.3.2"
  },
  "devDependencies": {
    "@types/node": "^25.0.9",
    "@types/react": "^19.2.8",
    "typescript": "^5.9.3"
  }
